<hmtl>
<head>
<title>Test Catalog and Documentation cross reference.</title>
</head>
<body>
<TMPL_IF name='directories'><h2>Sub-Directories</h2>
<table>
<tr valign="top">
<td>
<TMPL_LOOP name="directories">
    <a href='<TMPL_VAR name="title">'><TMPL_VAR name="title"></a><br />
    <TMPL_IF name="wrap"></td><td></TMPL_IF>
</TMPL_LOOP>
</td></tr>
</table>
</TMPL_IF>

<TMPL_IF name='Files'>
<h2>Files</h2>
<table>
<tr>
  <th>Links</th>
  <th style="color: #050">Pass</th>
  <th style="color: #500">Todo</th>
  <th style="color: #030">Fail</th>
  <th>File</th>
</tr>

<TMPL_LOOP name="files">
    <tr> 
       <td><TMPL_VAR name="links"></td>
       <td style="color: #050" title="ok"><TMPL_VAR name="ok"></td>
       <td style="color: #030" title="todo"><TMPL_VAR name="todo"></td>
       <td style="color: #500" title="failed"><TMPL_VAR name="failed"></td>
       <td><a href='<TMPL_VAR name='file'>.html'><TMPL_VAR name="file"></a></td>
    </tr>
</TMPL_LOOP>
</table>
</TMPL_IF>
</body>
</html>

