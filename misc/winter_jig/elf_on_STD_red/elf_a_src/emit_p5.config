CompUnit
local $whiteboard::in_package = [];
"\x23".'line 2 emitted_p5_'.int(rand(10000)).'
package main; # not Main, otherwise ::foo() hack for sub()s doesnt work.
use Data::Dumper;
'.join(";\n",@{$n<statements>})

Val_Int
$n<text>

Apply
if($n<code> =~ /^infix:(.+)$/) {
  my $op = $1;
  my($l,$r)=@{$n<arguments>};
  if($op eq '~'){ "($l . $r)" }
  elsif($op eq ','){ "$l, $r" }
  elsif($op eq '=') {
    my $t = $n->{arguments}[0]{twigil};
    if($t && $t eq '.') {
      $l.'('.$r.')'
    }
    else { "($l $op $r)" }
  }
  else { "($l $op $r)" }
}
elsif($n<code> =~ /^circumfix:(.+)/) {
  my $op = $1;
  my($arg)=join(",",@{$n<arguments>||[]});
  if(undef) {
  } else {
    $op =~ s/ /$arg/;
    $op  
  }
}
else {
  my $f = $n<code>;
  if($f =~ /^\$\w+$/) {
     $f.'->('.join(",",@{$n<arguments>}).')';
  }elsif($f eq 'self') {
    '$self'
  }elsif($f eq 'last') {
    'last'
  }elsif($f =~ /^\w/) {
     '::'.$f.'('.join(",",@{$n<arguments>}).')';
  }else{
     $f.'('.join(",",@{$n<arguments>}).')';
  }
}

Decl
if($n<decl> eq 'has') {
  my $default = $n<default>;
  if(defined $default) {
    $default = ", default => $default"
  } else {
    $default = ""
  }
  "has '".$n<var><name>."' => (is => 'rw'$default);"
}
elsif($n<var><twigil> eq '^') {
  my $name = $n<var>;
  $name =~ s/^(.)::/$1/;
  "{package main; use vars '".$name."'};".
  'local'.' '.$n<var>.($n<default> ? ' = '.$n<default> : '')
}
else {
  $n<decl>.' '.$n<var>.($n<default> ? ' = '.$n<default> : '')
}

Use
""

Val_Buf
