$s;

Val_Rx
'qr/'.$n<pat>.'/'

Var
my $s = $n<sigil>;
my $t = $n<twigil>||'';
#XXX $pkg:x -> s_pkg::x :(
my $env = '';
$env = 'e' if $t eq '^';
my $pre = '';
$pre = 's_' if $s eq '$' && $env eq 'e';
$pre = 'a_' if $s eq '@';
$pre = 'h_' if $s eq '%';
my $name = $env.$pre.$n<name>;
if($t eq '.') {
  '$self->'.$name
}elsif($t eq '^') {
  $name =~ s/::/__/g;
  '$'.'::'.$name
}else{
  '$'.$name
}

